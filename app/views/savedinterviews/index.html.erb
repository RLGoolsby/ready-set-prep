<div id="columns">
  <% @companies.each do |p| %>
  <figure>
    <%= button_to 'Go!', {:controller => "interview", :action => "create", :positions => p.position, :q=> p.company} , :method=>:post  %>
    <%= p.position %>
    <%= p.company %>
    <%= button_to 'x', {:controller => "savedinterviews", :id => p.id, :action => "destroy" }, {:method => :delete} %>

  </figure>
  <% end %>

</div>
